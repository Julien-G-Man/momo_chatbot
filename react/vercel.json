{
  "version": 2,
  "name": "momobot-cg",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_BASE_API_URL": "https://momobot-cg-api.onrender.com"
  }
}
